{
    "name": "anourvalar/eloquent-file",
    "description": "Database (Eloquent) layer for files management",
    "keywords": ["anourvalar", "laravel", "eloquent", "file", "upload"],
    "homepage": "https://github.com/AnourValar/eloquent-file",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "anourvalar/eloquent-validation": "^3.15",
        "anourvalar/laravel-atom": "^2.18",
        "intervention/image": "^3.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.26",
        "squizlabs/php_codesniffer": "^3.7",
        "psalm/plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {"AnourValar\\EloquentFile\\": "src/app/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnourValar\\EloquentFile\\Providers\\EloquentFileServiceProvider"
            ],
            "aliases": {
                "EloquentFile": "AnourValar\\EloquentFile\\Facades\\EloquentFileFacade"
            }
        }
    }
}
